{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "probe-rs-debug",
        "request": "launch",
        "name": "probe_rs Executable launch example",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "probe-rs",
        "runtimeArgs": ["dap-server"],
        "connectUnderReset": true,
        "chip": "STM32G0B1CBTx",
        "probe": "0483:3748",
        "flashingConfig": {
          "flashingEnabled": true,
          "haltAfterReset": true,
          "formatOptions": {
            "format": "elf"
          }
        },
        "coreConfigs": [
          {
            "coreIndex": 0,
            "programBinary": "${workspaceFolder}/app/target/thumbv6m-none-eabi/debug/canio",
            "svdFile": "${workspaceFolder}/app/STM32G0B1.svd",
            "rttEnabled": true,
            "rttChannelFormats": [
              {
                "channelNumber": 0,
                "dataFormat": "Defmt",
                "showTimestamps": true,
              }
            ]
          }
        ],
        "env": {
          "RUST_LOG": "info"
        },
         // Info, Debug
        "consoleLogLevel": "Console"
      }
    ]
  }